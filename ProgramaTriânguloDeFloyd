import java.util.Scanner;
public class TrianguloDeFloyyd {
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		System.out.print("DIGITE UM NÃšMERO: ");
		int n = ler.nextInt();
		int r = 1;
		for (int i = 1; i <= n; i++) {
			for(int j = 1;  j <= i; j++) {
				System.out.print(r + " ");
				r++;
			}
			System.out.println("");
		}
	}
}
